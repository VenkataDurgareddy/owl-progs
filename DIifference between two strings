class Solution {
public:
    char findTheDifference(string s, string t) {
        int ssum=0;
        int tsum=0;
        for(int i=0;i<s.length();i++)
        {
            ssum+=(int)s[i];
        }
         for(int i=0;i<t.length();i++)
        {
            tsum+=(int)t[i];
        }
        int sub=tsum-ssum;
        return (char)sub;
    }
};
